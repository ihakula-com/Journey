<?xml version="1.0" encoding="utf-8"?>
<ScrollView 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:facebook="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    
    >
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >

    <TextView 
        android:id="@+id/tv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"   
        android:text="NutsPaySDK"    
        android:padding="5dip"
        android:background="@color/color_blue"
        android:textColor="@color/color_orange" 
        android:textStyle="bold|italic"
        android:visibility="gone"
        />
  
     <Button
        android:id="@+id/click_set_server"
        android:layout_below="@id/tv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dip"
        android:text="SET_SERVER"
        android:onClick="onSetServerClick"
        android:textColor="@drawable/f_text_selector" />
    
    <Button
        android:id="@+id/click_tv"
        android:layout_below="@id/click_set_server"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dip"
        android:text="LAUNCH_PAY"
        android:onClick="onLaunchPayClick"
        android:textColor="@drawable/f_text_selector" />

    <Button
        android:id="@+id/click_service"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/click_tv"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="LAUNCH_SERVICE"
		android:onClick="onLaunchServiceClick"
        android:textColor="@drawable/f_text_selector" />

    <Button
        android:id="@+id/click_regist"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/click_service"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="LAUNCH_REGIST"
		android:onClick="onLaunchRegClick"        
        android:textColor="@drawable/f_text_selector" />

    <Button
        android:id="@+id/click_login"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/click_regist"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="LAUNCH_LOGIN"
		android:onClick="onLaunchLoginClick"          
        android:textColor="@drawable/f_text_selector" />

    <Button
        android:id="@+id/click_serialno"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/click_login"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="LAUNCH_OBTAIN_SERIALNO"
		android:onClick="onGetSericalNoClick"             
        android:textColor="@drawable/f_text_selector" />

      <Button
        android:id="@+id/get_accounts"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/click_serialno"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="GET_ACCOUNTS"
		android:onClick="onGetAccountsClick"            
        android:textColor="@drawable/f_text_selector" />
 
	<Button
        android:id="@+id/consume"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/get_accounts"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="CONSUME"
		android:onClick="onConsumeClick"           
        android:textColor="@drawable/f_text_selector" />
      
    <Button
        android:id="@+id/click_billing"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_below="@id/consume"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="5dip"
        android:text="LAUNCH_BILLING"
		android:onClick="onBillingClick"            
        android:textColor="@drawable/f_text_selector" 
       	android:visibility="gone"
        />

    <TextView
        android:id="@+id/fb_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/click_billing"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="20dip"
        android:text="facebook登录状态 : 未登录"
        android:textColor="@color/color_blue" />

<!--     <com.facebook.widget.LoginButton
        android:id="@+id/facebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/fb_username"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dip"
        android:paddingBottom="20dip"
        android:background="@color/color_white"
        android:src="@drawable/f_icon" /> -->

</RelativeLayout>
</ScrollView>